<article class='article-preview flex flex-col'>
	<h1><%= article.title %> </h1>
	<p>
	  <%= article.body %>
	</p>
	<div>
		<% if user_signed_in? %>
		  <%= link_to "Edit", edit_article_path(article), method: :get %>
		  <%= link_to "Delete", article_path(article), method: :delete %>
		<% end %>
	</div>
</article>